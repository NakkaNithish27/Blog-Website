<%-include("partials/header")-%>
<button class="btn btn-secondary compose-button"><a href="/compose">Compose a post</a></button>
<h1>Home</h1>
<p><%=homeStartingContent%></p>
<%posts.forEach(function(post){%>
  <h1><%=post.postTitle%></h1>
  <p><%=post.postBody.substr(0,100)+"..."%>   <a href="posts/<%=post._id%>">Read More</a></p>

<%})%>

<%-include("partials/footer")-%>
